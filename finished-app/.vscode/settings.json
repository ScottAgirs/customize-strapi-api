{
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#000",
    "titleBar.inactiveForeground": "#000000CC",
    "titleBar.activeBackground": "#8c4bff",
    "titleBar.inactiveBackground": "#a87cf5",
    "activityBar.foreground": "#a87cf5",
    "activityBar.background": "#333644",
    "activityBar.inactiveForeground": "#38B8F0",
    "activityBarBadge.foreground": "#ffffff",
    "activityBarBadge.background": "#B838F0",
    "statusBar.foreground": "#262626",
    "statusBar.background": "#38B8F0",
    "statusBarItem.remoteBackground": "#B838F0"
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "prettier.disableLanguages": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
